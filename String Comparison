#include <stdio.h>
int strcmp( char* str1, char* str2)
{
    while((*str1) && (*str1==*str2))
    {
        str1++;
        str2++;
    }
    return(*str1==*str2);
}

int main() {
 char str1[]= "Hello";
 char str2[]= "world";

strcmp(str1,str2);

if(str1==str2){
    printf("Strings are equal");
}
else
{
    printf("Strings are not Equal");
}
    return 0;
}

Output:

Strings are not Equal 
