
#include <stdio.h>

int main() {
int rows,i,j;
printf("\nenter number of rows");
scanf("\n%d",&rows);
for(i=rows;i>=1;i--)
{
    for(j=1;j<=i;j++)
    {
        printf("%d",j);
    }
    printf("\n");
}
    return 0;
}
---------------------------------------------------------------------------------------------------------------------------------------------------------------------
Output:
enter number of rows 5

12345
1234
123
12
1
---------------------------------------------------------------------------------------------------------------------------------------------------------------------
