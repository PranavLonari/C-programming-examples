#include <stdio.h>
int main() {
int rows,i,j;
printf("\nenter number of rows");
scanf("\n%d",&rows);
for(i=1;i<=rows;i++)
{
    for(j=1;j<=i;j++)
    {
        printf("%d",j);
    }
    printf("\n");
}
    return 0;
}

---------------------------------------------------------------------------------------------------------------------------------------------------------------------
Output:
enter number of rows
5

1
12
123
1234
12345

-------------------------------------------------------------------------------------------------------------------------------------------------------------------
