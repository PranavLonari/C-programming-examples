    //printf("Hello world");
    int a,b;
printf("enter 2 numbers\n");

scanf("%d %d ",&a,&b);

if(a>=b)
{
    if(a==b)
    {
        printf("%d == %d",a,b);
    }
    else
    {
        printf(" %d > %d",a,b);
    }
    
}
else
{
        printf(" %d < %d",a,b);
}
    return 0;
}


