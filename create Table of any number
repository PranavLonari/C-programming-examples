#include <stdio.h>

int main() {
int i,n;
printf("enter a number\n");
scanf("%d",&n);
for(i=1;i<=10;i++)
{
    printf("\n%d * %d = %d",n,i,n*i);
}
    return 0;
}

Output:
enter a number
44
  
44 * 1 = 44
44 * 2 = 88
44 * 3 = 132
44 * 4 = 176
44 * 5 = 220
44 * 6 = 264
44 * 7 = 308
44 * 8 = 352
44 * 9 = 396
44 * 10 = 440
