#include <stdio.h>

int main() {
    int i, number;
    
    printf("Enter a number: "); // Prompt the user to enter a number
    scanf("%d", &number); // Read the input number from the user
    
    printf("Series of numbers from 1 to given input number\n"); // Print a message indicating the series of numbers
    
    // Iterate from 1 to the input number
    for (i = 1; i <= number; i++) {
        printf("%d\n", i); // Print the current number in the series
    }
    
    return 0;
}

output:
Enter a number: 5
Series of numbers from 1 to given input number:
1
2
3
4
5
