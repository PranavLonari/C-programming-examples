#include <stdio.h>

int main() {
    int num, i;

    // Read the number from the user
    printf("Enter a number: ");
    scanf("%d", &num);

    printf("Reversed series of numbers from %d:\n", num);

    // Print the reversed series of numbers
    for (i = num; i > 0; i--) {
        printf("%d\n", i);

    return 0;
}

output:
Enter a number: 5
Reversed series of numbers from 5:
5
4
3
2
1
